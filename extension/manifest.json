{
  "manifest_version": 3,
  "name": "WebDefender",
  "version": "0.2",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["jquery-3.1.1.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}